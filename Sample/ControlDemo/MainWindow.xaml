<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlDemo"
        xmlns:Views="clr-namespace:VisionPlatform.Views;assembly=VisionPlatform.Views" x:Class="ControlDemo.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300">
    <Grid>
        <StackPanel >
            <Button Margin="0,10" FontSize="21" Click="Button_Click" Content="打开标定窗口"/>
            <Button Margin="0,10" FontSize="21" Click="Button_Click_1" Content="打开相机选择窗口"/>
            <Button Margin="0,10" FontSize="21" Click="Button_Click_2" Content="打开相机配置窗口"/>
            <Button Margin="0,10" FontSize="21" Click="Button_Click_3" Content="打开参数配置窗口"/>
            <Button Margin="0,10" FontSize="21" Click="Button_Click_4" Content="打开场景配置窗口"/>
            <Button Margin="0,10" FontSize="21" Click="Button_Click_5" Content="打开场景管理窗口"/>
        </StackPanel>
    </Grid>
</Window>
